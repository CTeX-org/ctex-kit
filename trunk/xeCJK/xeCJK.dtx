    \xeCJK_inter_class_toks:nnn { Boundary } { Default }
      { \@@_listings_process_Default:N }
    \xeCJK_inter_class_toks:nnn { Boundary } { IVS }
      { \@@_listings_process_IVS:nN { \c_zero } }
