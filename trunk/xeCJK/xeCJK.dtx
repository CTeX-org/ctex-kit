                  { \use:n }
                  { \g_@@_punct_bound_width_tl \use_none:n }
              }
          }
          { \use:n }
          { \@@_punct_width_or_ratio:nN { bound } {#2} }
      }
    \dim_set:Nn \l_@@_tmp_dim
      {
        \bool_if:NTF \l_@@_enabled_hanging_bool
          { \use:n }
          { \dim_max:nn { \l_@@_margin_minimum_dim } }
