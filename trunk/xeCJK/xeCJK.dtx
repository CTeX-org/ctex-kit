    \@@_listings_append:nN {#1} #2
  }
\cs_new_protected_nopar:Npn \@@_listings_process_FullRight:nN #1#2
  {
    \lst@whitespacefalse
    \bool_if:NTF \l_@@_listings_letter_bool
      {
        \bool_if:nT
          {
            \int_compare_p:nNn \l_@@_listings_flag_int < \c_two &&
            \@@_punct_if_long_p:N #2
