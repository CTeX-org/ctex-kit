                            \xeCJK_if_last_node_p:n { CJK-space }
                          }
                          {
                            \xeCJK_remove_node:
                            \bool_if:NTF \l_@@_reserve_space_bool
                              { ~ } { \CJKglue }
                          }
                          { ~ }
                      }
                  }
              }
          }
      }
  }
