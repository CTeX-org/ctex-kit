        \xeCJK_make_node:n { CJK-nobreak } #2
      }
      { \@@_check_single_save:N #1 #2#3 }
  }
\tl_new:N \l_@@_check_single_cs_case_tl
\cs_generate_variant:Nn \tl_case:NnF { No }
