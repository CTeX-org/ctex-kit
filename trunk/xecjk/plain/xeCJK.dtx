      { \prg_return_false: } { \prg_return_true:  }
  }
\prg_set_conditional:Nnn \xeCJK_if_it_or_sl:n { p, T, F, TF }
  {
    \bool_if:nTF
      { \str_if_eq_p:xx { it } {#1} || \str_if_eq_p:xx { sl } {#1} }
      { \prg_return_true:  } { \prg_return_false: }
  }
