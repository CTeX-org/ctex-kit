\def\CTEXnullspace{0pt}
\def\CTEX@null{\kern\CTEXnullspace{}\kern\CTEXnullspace}
\def\CTEX@zero{零}
\def\CTEX@one{一}
\def\CTEX@two{二}
\def\CTEX@three{三}
\def\CTEX@four{四}
\def\CTEX@five{五}
\def\CTEX@six{六}
\def\CTEX@seven{七}
\def\CTEX@eight{八}
\def\CTEX@nine{九}
\def\CTEX@ten{十}
\def\CTEX@hundred{百}
\def\CTEX@thousand{千}
