\def\CTEX@todaysmall{~\the\year~年~\the\month~月~\the\day~日}
\def\CTEX@todaybig{%
  \CTEXdigits{\temp@year}{\the\year}%
  \CTEXnumber{\temp@month}{\the\month}%
  \CTEXnumber{\temp@day}{\the\day}%
  \temp@year{}年\temp@month{}月\temp@day{}日}

\endinput

